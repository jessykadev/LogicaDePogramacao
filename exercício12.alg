algoritmo "NumMaior"

var
    num1, num2: Real

inicio

    Escreva("Escreva um número: ")
    Leia(num1)
    Escreva("Escreva outro número: ")
    Leia(num2)
    Se (num1 > num2) Então
        Escreva ("O primeiro número é o maior")
    Senão
        Escreva ("O segundo é o número maior")
    FimSe
fimalgoritmo